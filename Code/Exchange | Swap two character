.model small
.stack 100h

.code
main proc
    
    mov ah,1  ; taking first value
    int 21h
    mov bl, al
    
    mov ah,1  ; taking second value
    int 21h
    mov bh, al
    
    
    xchg bl,bh  ; exchange/swap value
    
    mov ah,2   ; new line
    mov dl,10
    int 21h
    mov dl,13
    int 21h
    
    ; after swapping
    mov ah,2   
    mov dl,bl
    int 21h
    
    mov ah,2
    mov dl,bh
    int 21h
  
           
    exit:
    mov ah,4ch
    int 21h
    main endp
end main
