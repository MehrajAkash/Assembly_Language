
.model small
.stack 100h
.code

main proc
         
    mov ah, 1
    int 21h
    mov bl, al 
 
 ; New Line segment
    mov ah, 2
    mov dl, 10 ;Ascii value of new line is 10
    int 21h 
    mov dl, 13 ;move the cursor to the begining of the line
    int 21h 
       
    mov ah, 2
    mov dl, bl
    int 21h
    
    exit: 
    mov ah, 4ch
    int 21h
    main endp
end main
      
