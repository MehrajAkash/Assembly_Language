.model small
.stack 100h
.code

MAIN PROC
    MOV AH, 2        ; DOS function: display character
    MOV CX, 256      ; Loop counter (256 characters)
    MOV DL, 0        ; Start with ASCII code 0

DISPLAY_LOOP:
    INT 21H          ; Display character in DL
    INC DL           ; Next ASCII character
    DEC CX           ; Decrease counter
    JNZ DISPLAY_LOOP ; Repeat if CX â‰  0

    MOV AH, 4CH      ; Terminate program
    INT 21H
MAIN ENDP

END MAIN
