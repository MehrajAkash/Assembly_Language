.model small
.stack 100h
.data
a db 10,13,'for and $'
b db 10,13,'for or $'
c db 10,13,'for xor $'

.code
main proc
    mov ax,@data
    mov ds,ax
    
    mov ah,9
    lea dx,a
    int 21h
    
    mov bl,111b ; op1=111
    and bl,101b ; op2=101
    ; bl=101=5
    
    add bl,48 
    
    mov ah,2
    mov dl,bl
    int 21h
    
    or1: 
    mov ah,9
    lea dx,b
    int 21h
    
    mov bl,111b
    or bl,101b ;bl=111=7
    
    add bl,48
    
    mov ah,2
    mov dl,bl
    int 21h 
    
    xor1:
    mov ah,9
    lea dx,c
    int 21h
    
    mov bl,111b
    xor bl,101b ; bl=010=2
    
    add bl,48  
    
    mov ah,2
    mov dl,bl
    int 21h 
    
    exit:
    mov ah,4ch
    main endp
end main
